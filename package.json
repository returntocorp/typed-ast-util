  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/esprima": "^4.0.2",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "esprima": "^4.0.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "ts-mocha": "^6.0.0",
